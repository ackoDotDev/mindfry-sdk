{
  "name": "@mindfry/sdk",
  "version": "1.0.0",
  "description": "Official TypeScript SDK for MindFry - Memory with a Conscience.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./protocol": {
      "import": "./dist/protocol/index.js",
      "types": "./dist/protocol/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cluster-127/mindfry-sdk",
    "directory": "ts"
  },
  "keywords": [
    "mindfry",
    "database",
    "graph",
    "ephemeral",
    "cognitive",
    "sdk",
    "typescript",
    "mfbp",
    "protocol"
  ],
  "author": "Erdem Arslan <erdemarslan@ymail.com>",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  }
}
